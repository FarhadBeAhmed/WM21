<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_2sdp"
    android:elevation="2dp"
   >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/layout_border_list_genealogy"
        android:layout_margin="@dimen/_3sdp"
        android:padding="@dimen/_5sdp"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:gravity="start"
            android:layout_marginTop="@dimen/_2sdp"
            android:orientation="horizontal">
            <TextView

                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Shop ID "
                android:textSize="@dimen/_11sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_regular"
                android:textStyle="normal"
                />
            <TextView
                android:id="@+id/shop_id"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="zubayer1"
                android:textSize="@dimen/_12sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_medium"


                />



        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:gravity="start"
            android:layout_marginTop="@dimen/_2sdp"
            android:orientation="horizontal">
            <TextView

                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Shop Owner"
                android:textSize="@dimen/_11sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_regular"
                android:textStyle="normal"
                />
            <TextView
                android:id="@+id/shop_owner_id"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="Md. Zubayer Hossain"
                android:textSize="@dimen/_12sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_medium"


                />



        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:layout_marginTop="@dimen/_2sdp"
            android:orientation="horizontal">
            <TextView

                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Shop Name"
                android:textSize="@dimen/_11sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_regular"
                android:textStyle="normal"
                />
            <TextView
                android:id="@+id/shop_name_id"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="Zubayer Enterprise"
                android:textSize="@dimen/_12sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_medium"


                />



        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:layout_marginTop="@dimen/_2sdp"
            android:orientation="horizontal">
            <TextView

                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Shop Type"
                android:textSize="@dimen/_11sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_regular"
                android:textStyle="normal"
                />
            <TextView
                android:id="@+id/shop_type_id"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="Zonal Depo"
                android:textSize="@dimen/_12sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_medium"


                />



        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:layout_marginTop="@dimen/_2sdp"
            android:orientation="horizontal">
            <TextView

                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Contact"
                android:textSize="@dimen/_11sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_regular"
                android:textStyle="normal"
                />
            <TextView
                android:id="@+id/shop_contact_id"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="1711207663"
                android:textSize="@dimen/_12sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_medium"


                />



        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:layout_marginTop="@dimen/_2sdp"
            android:orientation="horizontal">
            <TextView

                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Shop Address"
                android:textSize="@dimen/_11sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_regular"
                android:textStyle="normal"
                />
            <TextView
                android:id="@+id/address_id"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="Shop no 9, Zahiruddin super market"
                android:textSize="@dimen/_12sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_medium"


                />



        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:layout_marginTop="@dimen/_2sdp"
            android:orientation="horizontal">
            <TextView

                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Sales RP"
                android:textSize="@dimen/_11sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_regular"
                android:textStyle="normal"
                />
            <TextView
                android:id="@+id/sales_rp_id"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textSize="@dimen/_12sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_medium"


                />



        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:layout_marginTop="@dimen/_2sdp"
            android:orientation="horizontal">
            <TextView

                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Commission"
                android:textSize="@dimen/_11sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_regular"
                android:textStyle="normal"
                />
            <TextView
                android:id="@+id/commission_id"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textSize="@dimen/_12sdp"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_medium"


                />



        </LinearLayout>




    </LinearLayout>



</androidx.cardview.widget.CardView>